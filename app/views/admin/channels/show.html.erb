<div class="panel-heading">
  <h4>Mostrando Canal</h4>
</div>

<div class="panel-body">
  <p id="notice"><%= notice %></p>

  <dl class="dl-horizontal">
    <dt><strong>Title:</strong></dt>
    <dd><%= @channel.title %></dd>
  </dl>

  <dl class="dl-horizontal">
    <dt><strong>Content:</strong></dt>
    <dd><%= @channel.content %></dd>
  </dl>

  <dl class="dl-horizontal">
    <dt><strong>Logo:</strong></dt>
    <dd><%= @channel.logo %></dd>
  </dl>
</div>

<div class="panel-footer">
  <%= link_to 'Edit', edit_admin_channel_path(@channel), class: 'btn btn-default' %>
  <%= link_to 'Back', admin_channels_path, class: 'btn btn-default' %>
</div>